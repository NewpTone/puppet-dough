[DEFAULT]
log=/var/log/dough/dough-api.log
host = <%=host%>
listen = <%=listen%>
dough_user_composite_port = 8098
dough_admin_composite_port = 8099
# Example sql_connection=mysql://someuser:somepasswd@localhost/dough
sql_connection=<%=sql_connection%>
nova_sql_connection="mysql://<%=nova_db_username%>:<%=nova_db_passwd%>@<%=nova_db_host%>/<%=nova_db_name%>"
region = <%=region%>
dough_token=<%=dough_token%>
memcache= <%=memcache%>
keystone_username=<%=keystone_username%>
keystone_password=<%=keystone_password%>
keystone_tenant_name=<%=keystone_tenant_name%>
keystone_auth_url=<%=keystone_auth_url%>
purchases_buffer=<%=purchases_buffer%>
# Example migrate_url=mysql://root:nova@localhost/
migrate_url=<%=migrate_url%>
migrate_databases=<%=migrate_databases%>
rabbitmq_addr=<%=rabbitmq_addr%>
rabbitmq_user=<%=rabbitmq_user%>
rabbitmq_passwd=<%=rabbitmq_passwd%>
loglevel=<%=dough_loglevel%>
